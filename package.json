    "@types/mathjax": "^0.0.37",
    "gsap": "^3.11.0",
    "highlight.js": "^11.6.0",
    "iframe-resizer": "^4.3.2",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "mathjax-full": "^3.2.2",
    "mobile-drag-drop": "^3.0.0-beta.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.17",
    "polyfill-library": "^4.4.0",
    "typed.js": "^2.0.12",
